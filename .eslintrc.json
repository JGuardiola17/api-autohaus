{
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["warn", { "printWidth": 100, "singleQuote": true, "parser": "flow" }],
    "no-underscore-dangle": 0
  }
}
